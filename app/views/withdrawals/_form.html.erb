<%= form_for([@cryptocoin, @withdrawal]) do |f| %>
    <%= display_cryptocoin_fields(@cryptocoin, f) %>
    <br></br>
    <%= image_tag "pixel_coin_stack.png", style: 'height:3%;width:3%;' %>
      <%= f.label "Amount" %>
      <%= f.number_field :amount %>
      <%= f.submit class: 'button' %>
    <% end %>